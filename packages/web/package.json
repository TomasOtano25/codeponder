    "downshift": "^3.1.8",
    "formik": "^1.3.2",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.11",
    "lodash.get": "^4.4.2",
    "next": "^7.0.2",
    "next-routes": "^1.4.2",
